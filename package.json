{
  "name": "@dynamicabot/signales",
  "version": "2.1.1",
  "description": "Highly configurable logging utility",
  "license": "MIT",
  "repository": "dynamicabot/signales",
  "scripts": {
    "test": "tsd",
    "build": "tsup",
    "release": "pnpm publish && pnpm changeset tag"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "license.md"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/main.js",
      "import": "./dist/main.mjs",
      "types": "./dist/main.d.ts"
    }
  },
  "keywords": [
    "log",
    "cli",
    "logger",
    "logging",
    "hackable",
    "colorful",
    "console"
  ],
  "tsd": {
    "directory": "test-d"
  },
  "dependencies": {
    "chalk": "^5.1.2",
    "figures": "^6.0.0",
    "strip-ansi": "^7.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@types/node": "^20.0.0",
    "tsd": "^0.29.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@9.2.0",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Klaus Sinani",
    "email": "klaussinani@gmail.com",
    "url": "https://klaussinani.github.io"
  },
  "maintainers": [
    {
      "name": "Sebastian Pietschner",
      "email": "sebastian@sebasptsch.dev",
      "url": "https://www.sebasptsch.dev"
    }
  ]
}
